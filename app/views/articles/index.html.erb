<h1>List of all</h1>
<p>
<%= link_to "Create new article", new_article_path %>
</p>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>
  
  <% @articles.each do |stuff| %>
  <tr>
    <td><%= stuff.title %></td>
    <td><%= stuff.description %></td>
    <td><%= link_to "Edit", edit_article_path(stuff) %></td> 
    <td><%= link_to "Show", article_path(stuff) %></td> 
    <td><%= link_to "DELETE", article_path(stuff), method: :delete, data: {confirm: "Are you sure?"}  %></td>
    
  </tr>
  
  <% end %>
    
  
</table>

<p></p>
<p></p>
<p>
<%= link_to 'Home', root_path %>
</p>